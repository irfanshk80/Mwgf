<?xml version="1.0"?>
<openerp>
	<data noupdate="1">

		<record id="group_customer_service" model="res.groups">
			<field name="name">Customer Service Officer</field>
		</record>

		<record id="group_call_center" model="res.groups">
			<field name="name">Call Center Agent</field>
		</record>
		
		<record id="group_operation_manager" model="res.groups">
			<field name="name">Operation Manager</field>
		</record>

		<record id="group_manager" model="res.groups">
			<field name="name">Mawgif COO</field>
		</record>

		<record id="cso_rule" model="ir.rule">
			<field name="name">Customer Service Officer Rule</field>
			<field ref="model_maw_claim" name="model_id" />
			<field name="domain_force">[('district.user_ids','in',[user.id]),('state','not in',['new','opened'])]</field>
			<field name="groups" eval="[(4, ref('mawgif_support.group_customer_service'))]" />
		</record>

		<record id="cc_agent_rule" model="ir.rule">
			<field name="name">Call Center Agent Rule</field>
			<field ref="model_maw_claim" name="model_id" />
			<field name="domain_force">[(1,'=',1)]</field>
			<field name="groups" eval="[(4, ref('mawgif_support.group_call_center'))]" />
		</record>
		
		<record id="operation_manager_rule" model="ir.rule">
			<field name="name">Mawgif Operation Manager</field>
			<field ref="model_maw_claim" name="model_id" />
			<field name="domain_force">[('district.manager','=',user.id)]</field>
			<field name="groups" eval="[(4, ref('mawgif_support.group_operation_manager'))]" />
		</record>
		
		<record id="coo_agent_rule" model="ir.rule">
			<field name="name">COO Rule</field>
			<field ref="model_maw_claim" name="model_id" />
			<field name="domain_force">[(1,'=',1)]</field>
			<field name="groups" eval="[(4, ref('mawgif_support.group_manager'))]" />
		</record>
		
		<record id="group_no_messgng" model="res.groups">
            <field name="name">No Messaging</field>
        </record>

        <record id="mail.mail_feeds_main" model="ir.ui.menu">
            <field name="groups_id" eval="[(6,0,[ref('group_no_messgng')])]"/>
        </record>
        
        <record id="website.menu_website" model="ir.ui.menu">
			<field name="groups_id" eval="[(6,0,[ref('group_no_messgng')])]"/>
        </record>
		

	</data>

</openerp>